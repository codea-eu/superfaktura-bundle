{
  "name": "codea/superfaktura-bundle",
  "description": "",
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Edo Barnas",
      "email": "iam@edobarnas.com"
    }
  ],
  "config": {
    "platform": {
      "php": "8.1.0",
      "ext-json": "1",
      "ext-intl": "1"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Codea\\Bundle\\Superfaktura\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Codea\\Bundle\\Superfaktura\\Test\\": "tests/"
    }
  },
  "require": {
    "php": "^8.1",
    "symfony/framework-bundle": "6.*",
    "symfony/http-client": "6.*",
    "codea/superfaktura": "dev-master"
  },
  "require-dev": {
    "phpstan/phpstan": "1.3.*",
    "phpstan/phpstan-phpunit": "1.0.*",
    "phpunit/phpunit": "9.5.*",
    "symplify/easy-coding-standard": "10.0.*"
  }
}
